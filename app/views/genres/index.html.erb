<h1> Genres </h1>

<% @genres.each do |genre| %>
    <%= link_to genre_path(genre) do %>
        <h2> <%=genre.name%></h2>
    <% end %>
    <% genre.songs.each do |song| %>
        <h4> <%=song.name %> by <%=song.artist.name%>  </h4>
    <% end %>
<% end %>

<%= link_to new_genre_path do %>
    <div class="button">Create New Genre </div>
<% end %>