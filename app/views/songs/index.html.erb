<h1> Songs </h1>
<div class="songs_container">
<% @songs.each do |song| %>
    <%= link_to song_path(song) do %>
        <div class="song_container">    
            <p><%= song.name %></p>
            <h5><%= song.artist.name %></h5>
            <h5><%= song.genre.name %></h5>
        </div>
    <% end %>
<% end %>

